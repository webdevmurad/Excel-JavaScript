{
    "parser" : "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "linebreak-style": "off",
        "indent": "off",
        "quotes": "off",
        "no-trailing-spaces": "off",
        "eol-last": "off",
        "require-jsdoc": "off",
        "padded-blocks": "off",
        "operator-linebreak": "off",
        "max-len":"off",
        "new-cap": "off",
        "no-debugger": "off",
    },
    "extends": ["eslint:recommended", "google"]
}